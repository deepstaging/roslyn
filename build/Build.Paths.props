<Project>
  <!-- 
    Build.Paths.props
    
    Controls where NuGet packages are output.
    
    Imported by: Directory.Build.props
  -->
  
  <PropertyGroup>
    <!-- 
      PackageOutputPath: Where NuGet packages (.nupkg) go when you run 'dotnet pack'
    -->
    <PackageOutputPath>$(MSBuildThisFileDirectory)../../../artifacts/packages/</PackageOutputPath>
  </PropertyGroup>
</Project>
